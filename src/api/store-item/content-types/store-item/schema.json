{
  "kind": "collectionType",
  "collectionName": "store_items",
  "info": {
    "singularName": "store-item",
    "pluralName": "store-items",
    "displayName": "StoreItem"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "item": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::item.item"
    },
    "store": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::store.store"
    },
    "storeLocation": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::coordinate.coordinate"
    },
    "stockAmount": {
      "type": "integer",
      "required": true,
      "min": 0,
      "default": 0
    },
    "barcode": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true,
      "min": 0
    }
  }
}
