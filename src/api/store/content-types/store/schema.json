      "type": "float",
      "required": true
    },
    "latitude": {
      "type": "float",
      "required": true
    },
    "store_layout": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::store-layout.store-layout",
      "inversedBy": "store"
    }
  }
}
